	<package name="usermanage" extends="struts-default" namespace="/aa">
		<action name="user_*" class="com.zufangwang.action.UserAction"
			method="{1}">
			<result name="loginok_zuke">/main/zukemain.jsp</result>
			<result name="loginok_fangdong">/main/fangdongmain.jsp</result>
			<result name="login_admin">/admin/adminmain.jsp</result>
			<result name="error">/error.jsp</result>
