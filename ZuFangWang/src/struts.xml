			<result name="logout">/user/login_form.jsp</result>
			<result name="updateok">/common/myinfo.jsp</result>
			<!-- <result name="loginok_zuke" type="chain">querynewsAction</result> -->
		</action>
	</package>
	<!--DMI house增删改查 -->
	<package name="housemanage" extends="struts-default" namespace="/dd">
		<action name="house_*" class="com.zufangwang.action.HouseAction"
			method="{1}">
			<result name="publishok" type="redirect">/house/publishok.jsp</result>
			<result name="changeok" type="redirect">/house/updatehouse.jsp</result>
			<result name="xiangqingok">/house/houseinfo.jsp</result>
			<result name="queryMyHousesok">/house/myfz.jsp</result>
			<result name="cancelok">/house/myfz.jsp</result>
			<result name="error">/error.jsp</result>
		</action>
	</package>
<!--DMI order增删改查 -->
	<package name="ordermanage" extends="struts-default" namespace="/oo">
		<action name="order_*" class="com.zufangwang.action.OrderAction"
			method="{1}">
			<result name="insertorderok">/order/order_info.jsp</result>
			<result name="fdorders">/order/fdorders.jsp</result>
			<result name="zkorders">/order/zkorders.jsp</result>
			<result name="error">/error.jsp</result> 
		</action>
        <action name="admin_*" class="com.zufangwang.action.AdminAction" method="{1}">
         	<result name="queryallorder">/admin/admin_orders.jsp</result>
			<result name="deleteoneorder">/admin/admin_orders.jsp</result>
			<result name="queryalluser">/admin/admin_users.jsp</result>
			<result name="deleteoneuser">/admin/admin_users.jsp</result>
        </action>
	</package>
